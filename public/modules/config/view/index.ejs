<!DOCTYPE html>
<html ng-app="cartpool">
<head>
	<meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
	<title>cartpool</title>
	<!-- CSS asset files -->
	<link rel="stylesheet" type="text/css" href="/asset/bootstrap/dist/css/bootstrap.min.css">
	<!-- App CSS file -->
	<link rel="stylesheet" type="text/css" href="/public/dest/app.min.css">
	<!-- Angular Chart -->
	<link rel="stylesheet" href="/asset/angular-chart.js/dist/angular-chart.css">
	<!-- print -->
	<link rel="stylesheet" type="text/css" href="/asset/angular-print/angularPrint.css">
	<!-- view port -->
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
	<!-- required to remove the # symbole -->
	<base href="/">
</head>
<body>

	<header ng-include="'public/modules/config/view/nav.html'"></header>

	<section>
		<!-- get queries from the server, why? example is to get the last page the user has visited -->
		<% if(query){ %>
			<script type="text/javascript">
				window.query = <%-JSON.stringify(query)%>;
			</script>
		<% } %>
	</section>

	<div class="container">
		<div data-ui-view></div>
	</div>

	<footer ng-include="'public/modules/config/view/footer.html'"></footer>

</body>
	<!-- Lodash -->
	<script src="/asset/lodash/dist/lodash.min.js"></script>

	<!-- JQuery -->
	<script type="text/javascript" src="/asset/jquery/dist/jquery.min.js"></script>

	<!-- JS asset files -->
	<script type="text/javascript" src="/asset/angular/angular.min.js"></script>
	<script type="text/javascript" src="/asset/angular-resource/angular-resource.min.js"></script>
	<script type="text/javascript" src="/asset/angular-ui-router/release/angular-ui-router.min.js"></script>
	<!-- Angular animate -->
	<script src="/asset/angular-animate/angular-animate.min.js"></script>
	<!-- Angular Charts -->
	<script src="/asset/Chart.js/Chart.min.js"></script>
	<script src="/asset/angular-chart.js/dist/angular-chart.min.js"></script>
	<!-- Angular Timer -->
	<script src="/asset/momentjs/min/moment.min.js"></script>
	<script src="/asset/momentjs/min/locales.min.js"></script>
	<script src="/asset/humanize-duration/humanize-duration.js"></script>
	<script src="/asset/angular-timer/dist/angular-timer.min.js"></script>
	<!-- Angular QRCode -->
	<script src="asset/qrcode-generator/js/qrcode.js"></script>
	<script src="asset/angular-qrcode/angular-qrcode.js"></script>
	<!-- Bootstrap -->
	<script type="text/javascript" src="/asset/angular-bootstrap/ui-bootstrap.min.js"></script>
	<script type="text/javascript" src="/asset/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
	<!-- Dock Gird -->
	<script src="/asset/angular-deckgrid/angular-deckgrid.js"></script>
	<!-- print -->
	<script type="text/javascript" src="/asset/angular-print/angularPrint.js"></script>
	<!-- Scoket.io offline  -->
	<!-- <script src="public/offline/socket.io.js"></script>  -->
	<!-- socket.io  -->
	<script src="https://cdn.socket.io/socket.io-1.3.7.js"></script>
	<script src="asset/angular-socket-io/socket.min.js"></script>
	<!-- Angular Storage -->
	<script src="asset/a0-angular-storage/dist/angular-storage.min.js"></script>
	<!-- Angular JWT -->
	<script src="asset/angular-jwt/dist/angular-jwt.min.js"></script>
	<!-- App JS file -->
	<script type="text/javascript" type="text/javascript" src="/public/dest/app.min.js"></script>
</html>